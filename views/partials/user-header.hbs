              <div class="module module-cart pull-left">
                <div class="cart-icon">
                  <a href="/cart" style="color: #404040;"><i class="fa fa-shopping-cart"></i></a>
                  <span class="title">shop cart</span>
                  {{#gte cartCount '0'}}
                    <span class="cart-label" id="cartCount">{{cartCount}}</span>  
                  {{/gte}}
                  
                </div>
               
                <div class="cart-box">
                  <div class="cart-overview">
                    <ul class="list-unstyled">
                      {{#if Cartproducts}}
                       
                       {{#each Cartproducts}}
                      <li>
                        <img class="img-responsive" src="/uploads/{{this.item}}__1.jpg" alt="product" />
                        <div class="product-meta">
                          <h5 class="product-title">{{this.product.productname}}</h5>
                          <p class="product-price" style="color: brown;">â‚¹ {{this.price}}</p>
                          {{!-- <p class="product-quantity">Quantity: 2</p> --}}
                        </div>
                        <a class="cancel" onclick="deleteCartItem('{{this._id}}','{{this.product._id}}')">cancel</a>
                      </li>
                      {{/each}}

                      {{else}}
                      <h6 style="color: red;">The cart is empty</h6>
                    {{/if}}
                    </ul>
                  </div>
                  <div class="cart-total">
                    <div class="total-desc">
                      <h5></h5>
                    </div>
                    <div class="total-price">
                      <h5>{{products.price}}</h5>
                    </div>
                  </div>
                  <div class="clearfix"></div>
                  <div class="cart-control">
